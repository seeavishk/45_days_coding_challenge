//just return no. of bits for all the number less than n..return the number of 1's of the number..
class Solution {
public:
    vector<int> countBits(int n) {
     vector<int> ams;
     for(int i=0;i<=n;i++){
        int sum=0;
        int num=i;
        while(num!=0){
            sum+=num%2;
            num=num/2;
        }
        ans.push_back(sum);
     }   
     return ans;
    }
};
